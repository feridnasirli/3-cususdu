@model Team
@{
    ViewData["Title"] = "Create";
}

<form method="post" asp-action="create" enctype="multipart/form-data">

    <div class="col-md-5 form-group">
        <label asp-for="FullName" >FullName</label>
        <input asp-for="FullName" class="form-control"  >
    </div>

    <div class="col-md-5 form-group">
        <label asp-for="Pression">Pressioon</label>
        <input asp-for="Pression" class="form-control" >
    </div>

    <div class="col-md-5 form-group">
        <label asp-for="ImageFile">File input</label>
        <input asp-for="ImageFile" type="file">
        <span asp-validation-for="ImageFile"></span>
    </div>

    <div class="col-md-5 form-group">
        <label asp-for="InstagramURL">Instagram</label>
        <input asp-for="InstagramURL" class="form-control" >
    </div>

    <div class="col-md-5 form-group">
        <label asp-for="FacebookURL">FaceBook</label>
        <input asp-for="FacebookURL" class="form-control" >
    </div>

    <div class="col-md-5 form-group">
        <label asp-for="TwitterURL">Twitter</label>
        <input asp-for="TwitterURL" class="form-control" >
    </div>

    <button type="submit" class="btn btn-success">Create</button>
</form>
